{
  "name": "chartify",
  "description": "React.js plugin for creating charts. Inspired by kinopoisk.ru chart. Feel free for contribute.",
  "version": "3.0.1",
  "scripts": {
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "flow start": "flow start",
    "flow stop": "flow stop",
    "flow status": "flow status",
    "flow coverage": "flow coverage",
    "start": "webpack",
    "lint": "eslint src/Chartify.js --fix",
    "test": "jest",
    "prettier": "prettier src/*.js --ignore-path ./.prettierignore --write && git add . && git status",
    "prettier-watch": "onchange 'src/*.js' -- prettier --write {{changed}}",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "pre-commit": [
    "prettier",
    "lint"
  ],
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/assetsTransformer.js"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  },
  "dependencies": {
    "eslint": "^4.19.1",
    "eslint-config-standard": "^12.0.0-alpha.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-hot-loader": "^3.0.0-beta.6"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.0.0-alpha.3",
    "@storybook/addon-links": "^4.0.0-alpha.3",
    "@storybook/addons": "^4.0.0-alpha.3",
    "@storybook/react": "^4.0.0-alpha.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.2.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^22.0.4",
    "babel-loader": "^6.2.0",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.2.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.23.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "flow-bin": "^0.68.0",
    "jest": "^22.0.4",
    "jest-cli": "^22.0.4",
    "node-sass": "^3.4.2",
    "nyc": "^11.6.0",
    "postcss-cssnext": "^2.8.0",
    "postcss-loader": "^0.8.2",
    "postcss-mixins": "^5.0.0",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^3.0.0",
    "pre-commit": "^1.2.2",
    "precss": "^1.4.0",
    "prettier": "1.9.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "socket.io-client": "^2.0.4",
    "style-loader": "^0.13.0",
    "webpack": "^4.0.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^2.4.1",
    "webpack-uglify-js-plugin": "1.1.9"
  },
  "main": "chartify.js",
  "repository": "kis/chartify",
  "homepage": "github.com/kis/chartify",
  "keywords": [
    "react",
    "reactjs",
    "react-js",
    "react-chart",
    "scale",
    "items",
    "dataset",
    "kinopoisk",
    "chart",
    "line-chart",
    "bar-chart",
    "marks",
    "axis",
    "calendar-view",
    "pyramid",
    "progress-chart",
    "showreel",
    "visualisation",
    "diagram",
    "graph"
  ],
  "author": {
    "name": "Kirill Stepkin",
    "email": "kirillstyopkin@gmail.com",
    "url": "github.com/kis"
  },
  "bugs": {
    "url": "https://github.com/kis/chartify/issues",
    "email": "kirillstyopkin@gmail.com"
  },
  "license": "MIT"
}
